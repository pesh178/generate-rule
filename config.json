{
    "dns_servers": [
        "8.8.8.8",
        "1.1.1.1"
    ],
    "full_rule_outputs": [
        {
            "target_software": "clash",
            "output_name": "clash-premium-rules.yaml",
            "parts": [
                {
                    "type": "base",
                    "files": [
                        "./clash/clash-premium-template.yaml"
                    ]
                },
                {
                    "type": "surge-like-rules",
                    "minify": true,
                    "action_replace": {
                        "REJECT": "AdBlock",
                        "DIRECT": "DefaultAction"
                    },
                    "files": [
                        "./common-rules/common-rules.conf",
                        "./clash/clash-premium-rules-pre.conf",
                        "./common-rules/common-medium.conf",
                        "./clash/clash-premium-rules-medium.conf",
                        "./common-rules/common-final.conf"
                    ]
                }
            ]
        }
    ],
    "servers": [],
    "rules_list": [
        {
            "name": "AdGuard DNS Filter",
            "type": "adblock",
            "action_type": "HOST-SUFFIX",
            "action": "REJECT",
            "exclude_action": "IGNORE",
            "file_prefix": "adguard-dns",
            "url": "https://raw.githubusercontent.com/8680/GOODBYEADS/master/data/rules/adblock.txt"
        }
        {
            "name": "White AdGuard DNS Filter",
            "type": "adblock",
            "action_type": "HOST-SUFFIX",
            "action": "DIRECT",
            "exclude_action": "IGNORE",
            "file_prefix": "White-adguard-dns",
            "url": "https://raw.githubusercontent.com/8680/GOODBYEADS/master/data/rules/allow.txt"
        }
    ]
}
